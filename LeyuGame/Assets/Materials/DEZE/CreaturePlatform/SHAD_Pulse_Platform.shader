		_PulseGlow("PulseGlow", 2D) = "white" {}
		_GlowColor("GlowColor", Color) = (0.1529412,0.6509804,1,0)
		_GlowStrength("GlowStrength", Float) = 1
		[HideInInspector] _texcoord( "", 2D ) = "white" {}

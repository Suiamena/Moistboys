		Tags{ "RenderType" = "Overlay"  "Queue" = "Overlay+0" "IgnoreProjector" = "True" "IsEmissive" = "true"  }
		Cull Back
		Blend One One
		
		AlphaToMask On
		CGINCLUDE
		#include "UnityShaderVariables.cginc"
		#include "UnityPBSLighting.cginc"
		#include "Lighting.cginc"
		#pragma target 3.0
		struct Input
		{
			float2 uv_texcoord;
		};

		uniform float4 _GlowColor;
		uniform sampler2D _PulseGlow;
		uniform float _Pan_Speed;
		uniform float _GlowStrength;

		void surf( Input i , inout SurfaceOutputStandard o )
		{
			float2 temp_cast_0 = (_Pan_Speed).xx;
			float2 panner5 = ( 1.0 * _Time.y * temp_cast_0 + i.uv_texcoord);
			o.Emission = ( _GlowColor * tex2D( _PulseGlow, panner5 ) * _GlowStrength ).rgb;
